        function Plot3DCartesianPositionTrajectories(obj, origin, arg)
            switch origin
                case 'results'
                    i_trial = arg;
                    if (obj.ROSConnected)
                        if (i_trial~=0)
                            figure('WindowState', 'maximized', 'Name', 'Posición cartesiana 3D');
                            hold on;
                            obj.Plot3DCartesianPositionTrajectory(obj.DataInputShow,obj.ColorDataInput);
                            obj.Plot3DCartesianPositionTrajectory(obj.DataCommanded,obj.ColorDataCommanded);
                            obj.Plot3DCartesianPositionTrajectory(obj.DataOutput{i_trial},obj.ColorDataOutput);
                            view([-120, 30]);
                            axis equal;
                            legend('Grabada', 'Comandada', 'Ejecutada');
                            title('Trayectoria cartesiana en 3D');
                        else
                            obj.CalculateMinMeanMax(obj.DataOutput);
                            figure('WindowState', 'maximized', 'Name', 'Repetibilidad cartesiana 3D');
                            hold on
                            for i = 1:max(size(obj.DataOutput))
                                obj.Plot3DCartesianPositionTrajectory(obj.DataOutput{i},obj.ColorDataOutput);
                            end
                            obj.Plot3DCartesianPositionTrajectory(obj.DataCommanded{1}, obj.ColorDataCommanded);
                            view([-120, 30]);
                            axis equal;
                            title('Trayectoria cartesiana en 3D');
                            f = get(gca, 'Children');
                            legend([f(1), f(2)], 'Comandada', 'Ejecutada')
%                             ME=MException('IIWAControl:Plot3DRepeatibility', 'Plot3DRepeatibility not implemented yet');
%                             throw(ME);
                        end
                    end
                case 'database'
                    Trajectory = arg;
                    if(~isempty(Trajectory))
                        [trajectoryOutput, trajectoryCommanded] = IIWAControl.SetTrajectoryFromFile(Trajectory);
